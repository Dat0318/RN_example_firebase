{
  "name": "rn_example_base",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "m1": "sudo arch -x86_64 gem install ffi && arch -x86_64",
    "pod": "cd ios && pod install && cd ..",
    "clean": "cd android && ./gradlew clean && cd .. && npm cache clean --force",
    "clean_ios": "cd ios && xcodebuild clean && cd ..",
    "clean_all": "react-native-clean-project",
    "apk": "cd android && ./gradlew assembleRelease && cd ..",
    "aab": "cd android &&./gradlew clean && ./gradlew bundleRelease && cd ..",
    "cpis": "appcenter codepush release-react -a adamodigital/ShopStreamIos -d Staging",
    "cpas": "appcenter codepush release-react -a adamodigital/ShopStreamAndroid -d Staging",
    "cpip": "appcenter codepush release-react -a adamodigital/ShopStreamIos -d Production",
    "cpap": "appcenter codepush release-react -a adamodigital/ShopStreamAndroid -d Production",
    "ip": "ipconfig getifaddr en0",
    "cpcp": "appcenter codepush deployment clear -a adamodigital/ShopStreamAndroid Production && appcenter codepush deployment clear -a adamodigital/ShopStreamIos Production",
    "cpcs": "appcenter codepush deployment clear -a adamodigital/ShopStreamAndroid Staging && appcenter codepush deployment clear -a adamodigital/ShopStreamIos Staging",
    "postinstall": "npx jetify"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.1.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/analytics": "^12.0.0",
    "@react-native-firebase/app": "^12.0.0",
    "@react-native-firebase/auth": "^12.0.0",
    "@react-native-firebase/crashlytics": "^12.0.0",
    "@react-native-firebase/database": "^12.0.0",
    "@react-native-firebase/dynamic-links": "^12.0.0",
    "@react-native-firebase/firestore": "^12.0.0",
    "@react-native-firebase/functions": "^12.0.0",
    "@react-native-firebase/in-app-messaging": "^12.0.0",
    "@react-native-firebase/messaging": "^12.0.0",
    "@react-native-firebase/ml": "^12.0.0",
    "@react-native-firebase/perf": "^12.0.0",
    "@react-native-firebase/remote-config": "^12.0.0",
    "@react-native-firebase/storage": "^12.0.0",
    "@react-native-google-signin/google-signin": "^6.0.1",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.14.5",
    "add": "^2.0.6",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-fetch-blob": "^0.10.8",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^3.4.0",
    "react-native-twitter-signin": "^1.1.1",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.24",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^27.0.6",
    "eslint": "^7.14.0",
    "jest": "^27.0.6",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
